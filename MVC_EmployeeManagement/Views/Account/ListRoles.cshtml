@model IEnumerable<MVC_EmployeeManagement.ViewModels.RoleVm>

@{ 
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Roles</h1>

<div class="card">
    @foreach (var role in Model)
    {
        <div class="card-header">
            <h1>Role</h1>
        </div>
        <div class="card-body">

            <h3>@role.Name</h3>

        </div>
    }
</div>